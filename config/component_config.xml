<?xml version="1.0" encoding="utf-8"?>
<运行状态监视:>
        <参数设置:>
                <公司编码>
                        <cmd>set_company_no</cmd>
                        <regex>^[1-9]\d*$</regex>
                </公司编码>
                <逻辑地址>
                        <cmd>set_logical_addr</cmd>
                        <int>0,</int>
                </逻辑地址>
                <服务器ip>
                        <cmd>set_server_ip</cmd>
                        <regex>(25[0-5]|2[0-4]\d|[0-1]\d{2}|[1-9]?\d)\.(25[0-5]|2[0-4]\d|[0-1]\d{2}|[1-9]?\d)\.(25[0-5]|2[0-4]\d|[0-1]\d{2}|[1-9]?\d)\.(25[0-5]|2[0-4]\d|[0-1]\d{2}|[1-9]?\d)</regex>
                </服务器ip>
                <心跳周期>
                        <cmd>set_heartbeat_period</cmd>
                        <int>0,</int>
                </心跳周期>
        </参数设置:>
        <参数读取:>
                <公司编码>
                        <cmd>get_company_no</cmd>
                </公司编码>
                <逻辑地址>
                        <cmd>get_logical_addr</cmd>
                </逻辑地址>
                <服务器ip>
                        <cmd>get_server_ip</cmd>
                </服务器ip>
                <心跳周期>
                        <cmd>get_heartbeat_period</cmd>
                </心跳周期>
                <温湿度读取>
                        <cmd>get_temp</cmd>
                </温湿度读取>
                <读取运行参数>
                        <cmd>get_dynamic_args</cmd>
                </读取运行参数>
        </参数读取:>
        <设备控制:>
                <终端复位重启>
                        <cmd>reset</cmd>
                </终端复位重启>
                <终端断电重启>
                        <cmd>power_off</cmd>
                </终端断电重启>
                <电源控制>
                        <cmd>power_act</cmd>
                        <enum>on,off</enum>
                </电源控制>
                <风扇控制>
                        <cmd>fan_act</cmd>
                        <enum>on,off</enum>
                </风扇控制>
                <led控制:>
                        <led开>
                                <cmd>led_act</cmd>
                                <enum>on</enum>
                        </led开>
                        <led关>
                                <cmd>led_off</cmd>
                                <enum>off</enum>
                        </led关>
                </led控制:>
        </设备控制:>
</运行状态监视:>
